#root-tag-container {
    align-self: flex-start;
    align-items: center;
    flex-direction: row;
    flex-grow: 0;
    flex-shrink: 0;
    flex-basis: auto;
    justify-content: center;
    -unity-slice-left: 100;
    -unity-slice-top: 100;
    -unity-slice-right: 100;
    -unity-slice-bottom: 100;
    background-image: url("project://database/Assets/UnderdorkStudios/UnderTags/Sprites/9Sliced.png?fileID=2800000&guid=fb6aea370a739bf39950a94e32dd8cad&type=3#9Sliced");
    -unity-background-image-tint-color: var(--unity-colors-button-background);
    -unity-slice-scale: 0.05px;
    margin-right: 7.5px;
    margin-bottom: 5px;
    margin-top: 5px;
    margin-left: 7.5px;
}

#tag-label {
    color: var(--unity-colors-button-text);
    margin-left: 10px;
    margin-right: 10px;
    flex-shrink: 0;
    flex-direction: row;
    flex-grow: 1;
    align-self: center;
    font-size: var(--unity-metrics-default-font_normal_size);
    height: 25px;
    -unity-text-align: middle-left;
}

#test {
}

#remove-tag-button {
    width: 17.5px;
    height: 17.5px;
    -unity-background-image-tint-color: rgb(180, 81, 81);
    background-image: url("project://database/Assets/UnderdorkStudios/UnderTags/Sprites/RemoveIcon.png?fileID=2800000&guid=25011a4e5dd2145abaf912eb447cf5f2&type=3#RemoveIcon");
    color: rgb(255, 255, 255);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    margin-left: 0;
    margin-right: 7px;
}
