#root-container {
    flex-grow: 0;
    flex-shrink: 0;
    align-items: flex-start;
    justify-content: flex-start;
    flex-basis: auto;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    align-self: auto;
    flex-direction: column;
    flex-wrap: nowrap;
}

#tag-operations-container {
    flex-shrink: 0;
    flex-direction: row;
    justify-content: space-between;
    align-self: stretch;
    align-items: stretch;
    margin-bottom: 10px;
    margin-top: 8px;
}

#toggle-add-tag-container-button {
    rotate: 0;
    width: 20px;
    height: 20px;
    flex-shrink: 0;
    align-items: flex-start;
    justify-content: flex-start;
    align-self: center;
    margin-top: 0;
    margin-bottom: 5px;
    background-image: url("project://database/Assets/UnderdorkStudios/UnderTags/Sprites/AddIcon.png?fileID=2800000&guid=cd428f4aad1d94043a7d30a692ef6412&type=3#AddIcon");
    background-color: rgba(0, 0, 0, 0);
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0;
}

#tag-search-input-field {
    align-self: stretch;
    align-items: stretch;
    justify-content: flex-start;
    width: 125px;
    display: none;
}

#owned-tag-root-container {
    align-self: stretch;
}

#owned-tag-label {
    font-size: var(--unity-metrics-default-font_normal_size);
    color: var(--unity-colors-default-text);
    margin-left: 3px;
    margin-bottom: 8px;
    -unity-font-style: bold;
}

#owned-tag-container {
    flex-direction: row;
    flex-wrap: wrap;
    flex-shrink: 0;
    align-self: auto;
    align-items: flex-start;
    justify-content: flex-start;
    border-top-width: 0.5px;
    border-right-width: 0.5px;
    border-bottom-width: 0.5px;
    border-left-width: 0.5px;
    border-left-color: rgb(31, 31, 31);
    border-right-color: rgb(31, 31, 31);
    border-top-color: rgb(31, 31, 31);
    border-bottom-color: rgb(31, 31, 31);
    margin-left: 3px;
    margin-bottom: 10px;
}

#no-tags-added-container {
    align-items: flex-start;
    justify-content: center;
}

.toggle-add-tag-container-button-active {
    display: flex;
    -unity-background-image-tint-color: rgb(173, 122, 87);
    transition-duration: 0.2s;
    transition-timing-function: ease-in-out;
    transition-property: -unity-background-image-tint-color;
}

.toggle-add-tag-container-button-inactive {
    display: flex;
    -unity-background-image-tint-color: rgb(85, 144, 71);
    transition-property: -unity-background-image-tint-color;
    transition-duration: 0.2s;
    transition-timing-function: ease-in-out;
}
